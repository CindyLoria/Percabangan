#include <iostream>
using namespace std;

int main(){
	  
    // Deklarasi tipe data dan variabel
    int bil, jum, i;
    
        cout << "Masukkan bilangan: ";
        cin >> bil;
    jum = 0;
   
        for (i=1; i<=bil; i++){
            if (bil%i==0){
                jum++;
            }
        }
       
        if (jum==2){
            cout <<bil<<" "<<"Adalah Bilangan Prima \n ";
           
        }
        else{
            cout <<bil<<" "<<"Bukan bilangan Prima \n ";
           
        }
        return 0;
}
